        "php": ">=8.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-redis": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
