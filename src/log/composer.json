{
    "name": "larmias/log",
    "description": "PSR-3 based log library, supporting console and file output",
    "type": "library",
    "keywords": [
        "larmias",
        "log",
        "logger"
    ],
    "homepage": "https://larmias.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "larmdcm",
            "email": "mdcm98@163.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "larmias/contracts": "^1.0",
        "larmias/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Larmias\\Log\\": "src/"
        }
    },
    "extra": {
        "larmias": {
            "providers": [
                "Larmias\\Log\\Providers\\LogServiceProvider"
            ]
        }
    }
}