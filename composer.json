{
  "name": "larmias/larmias",
  "description": "A memory resident php development framework.",
  "type": "library",
  "keywords": [
    "php",
    "workerman",
    "swoole",
    "larmias",
    "psr",
    "framework"
  ],
  "homepage": "https://larmias.com/",
  "support": {
    "issues": "https://github.com/lardmcm/larmias/issues",
    "source": "https://github.com/lardmcm/larmias"
  },
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "larmdcm",
      "email": "mdcm98@163.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-mbstring": "*",
    "ext-ctype": "*",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "ext-redis": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "psr/container": "^2.0",
    "psr/log": "^3.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-client": "^1.0",
    "psr/simple-cache": "^1.0",
    "opis/closure": "^3.6",
    "nikic/fast-route": "^1.3",
    "workerman/workerman": "^4.1",
    "godruoyi/php-snowflake": "^1.0",
    "nesbot/carbon": "^2.0",
    "symfony/console": "^6.0",
    "symfony/finder": "^4.3.2|^5.1"
  },
  "require-dev": {
    "swoole/ide-helper": "^4.5",
    "phpunit/phpunit": "^9.6"
  },
  "replace": {
    "larmias/async-queue": "self.version",
    "larmias/auth": "self.version",
    "larmias/cache": "self.version",
    "larmias/captcha": "self.version",
    "larmias/command": "self.version",
    "larmias/config": "self.version",
    "larmias/contracts": "self.version",
    "larmias/crontab": "self.version",
    "larmias/database": "self.version",
    "larmias/di": "self.version",
    "larmias/encryption": "self.version",
    "larmias/engine": "self.version",
    "larmias/engine-swoole": "self.version",
    "larmias/engine-workerman": "self.version",
    "larmias/enum": "self.version",
    "larmias/env": "self.version",
    "larmias/event": "self.version",
    "larmias/exception-handler": "self.version",
    "larmias/file-watcher": "self.version",
    "larmias/framework": "self.version",
    "larmias/http-csrf": "self.version",
    "larmias/http-message": "self.version",
    "larmias/http-server": "self.version",
    "larmias/http-utils": "self.version",
    "larmias/lock": "self.version",
    "larmias/log": "self.version",
    "larmias/middleware": "self.version",
    "larmias/paginator": "self.version",
    "larmias/pipeline": "self.version",
    "larmias/pool": "self.version",
    "larmias/process": "self.version",
    "larmias/redis": "self.version",
    "larmias/routing": "self.version",
    "larmias/session": "self.version",
    "larmias/shared-memory": "self.version",
    "larmias/snowflake": "self.version",
    "larmias/task": "self.version",
    "larmias/throttle": "self.version",
    "larmias/timer": "self.version",
    "larmias/translation": "self.version",
    "larmias/utils": "self.version",
    "larmias/validation": "self.version",
    "larmias/view": "self.version"
  },
  "suggest": {
    "ext-swoole": "Required to use swoole (^4.6.*)."
  },
  "autoload": {
    "files": [
      "src/utils/src/functions.php",
      "src/engine/src/functions.php",
      "src/framework/src/functions.php",
      "src/view/src/Blade/helpers.php",
      "src/http-server/src/functions.php",
      "src/http-csrf/src/functions.php"
    ],
    "psr-4": {
      "Larmias\\Utils\\": "src/utils/src/",
      "Larmias\\Contracts\\": "src/contracts/src/",
      "Larmias\\Config\\": "src/config/src/",
      "Larmias\\Cache\\": "src/cache/src/",
      "Larmias\\Di\\": "src/di/src/",
      "Larmias\\Event\\": "src/event/src/",
      "Larmias\\Engine\\": "src/engine/src/",
      "Larmias\\Engine\\WorkerMan\\": "src/engine-workerman/src/",
      "Larmias\\Engine\\Swoole\\": "src/engine-swoole/src/",
      "Larmias\\SharedMemory\\": "src/shared-memory/src/",
      "Larmias\\Http\\Message\\": "src/http-message/src/",
      "Larmias\\HttpServer\\": "src/http-server/src/",
      "Larmias\\Routing\\": "src/routing/src/",
      "Larmias\\Middleware\\": "src/middleware/src/",
      "Larmias\\Pipeline\\": "src/pipeline/src/",
      "Larmias\\ExceptionHandler\\": "src/exception-handler/src/",
      "Larmias\\Log\\": "src/log/src/",
      "Larmias\\Command\\": "src/command/src/",
      "Larmias\\Framework\\": "src/framework/src/",
      "Larmias\\Env\\": "src/env/src/",
      "Larmias\\Validation\\": "src/validation/src/",
      "Larmias\\Translation\\": "src/translation/src/",
      "Larmias\\Redis\\": "src/redis/src/",
      "Larmias\\Lock\\": "src/lock/src/",
      "Larmias\\Session\\": "src/session/src/",
      "Larmias\\View\\": "src/view/src/",
      "Larmias\\Crontab\\": "src/crontab/src/",
      "Larmias\\Http\\CSRF\\": "src/http-csrf/src/",
      "Larmias\\Http\\Utils\\": "src/http-utils/src/",
      "Larmias\\Captcha\\": "src/captcha/src/",
      "Larmias\\Snowflake\\": "src/snowflake/src/",
      "Larmias\\Task\\": "src/task/src/",
      "Larmias\\Auth\\": "src/auth/src/",
      "Larmias\\Encryption\\": "src/encryption/src/",
      "Larmias\\Enum\\": "src/enum/src/",
      "Larmias\\AsyncQueue\\": "src/async-queue/src/",
      "Larmias\\Throttle\\": "src/throttle/src/",
      "Larmias\\Process\\": "src/process/src/",
      "Larmias\\Timer\\": "src/timer/src/",
      "Larmias\\Pool\\": "src/pool/src/",
      "Larmias\\FileWatcher\\": "src/file-watcher/src/",
      "Larmias\\Database\\": "src/database/src/",
      "Larmias\\Paginator\\": "src/paginator/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larmias\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}