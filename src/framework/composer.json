{
    "name": "larmias/framework",
    "description": "The framework of Larmias.",
    "type": "library",
    "keywords": [
        "larmias",
        "framework"
    ],
    "homepage": "https://larmias.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "larmdcm",
            "email": "mdcm98@163.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/log": "^3.0",
        "larmias/contracts": "^1.0",
        "larmias/engine": "^1.0",
        "larmias/command": "^1.0",
        "larmias/config": "^1.0",
        "larmias/event": "^1.0",
        "larmias/support": "^1.0",
        "larmias/env": "^1.0",
        "larmias/process": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Larmias\\Framework\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "larmias": {
            "providers": [
                "Larmias\\Framework\\Providers\\BootServiceProvider"
            ]
        }
    }
}