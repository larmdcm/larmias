{
    "name": "larmias/http-server",
    "description": "Http server library based on PSR-7 / PSR-17 standard.",
    "type": "library",
    "keywords": [
        "larmias",
        "http",
        "server",
        "psr",
        "psr-7"
    ],
    "homepage": "https://larmias.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "larmdcm",
            "email": "mdcm98@163.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "larmias/contracts": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "larmias/http-message": "^1.0",
        "larmias/utils": "^1.0",
        "larmias/routing": "^1.0",
        "larmias/middleware": "^1.0",
        "larmias/exception-handler": "^1.0"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Larmias\\HttpServer\\": "src/"
        }
    },
    "extra": {
        "larmias": {
            "providers": [
                "Larmias\\HttpServer\\Providers\\HttpServiceProvider"
            ]
        }
    }
}