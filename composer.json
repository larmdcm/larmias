{
    "name": "larmias/larmias",
    "description": "A memory resident php development framework.",
    "type": "library",
    "keywords": [
        "php",
        "workerman",
        "swoole",
        "larmias",
        "psr",
        "framework"
    ],
    "homepage": "https://larmias.com/",
    "support": {
        "issues": "https://github.com/larmdcm/larmias/issues",
        "source": "https://github.com/larmdcm/larmias"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "larmdcm",
            "email": "mdcm98@163.com"
        }
