{
    "name": "larmias/http-csrf",
    "description": "A http-csrf component for larmias.",
    "type": "library",
    "keywords": [
        "larmias",
        "http",
        "csrf"
    ],
    "homepage": "https://larmias.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "larmdcm",
            "email": "mdcm98@163.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "larmias/contracts": "^1.0",
        "larmias/utils": "^1.0"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Larmias\\Http\\CSRF\\": "src/"
        }
    },
    "extra": {
        "larmias": {
            "providers": [
                "Larmias\\Http\\CSRF\\Providers\\CsrfServiceProvider"
            ]
        }
    }
}