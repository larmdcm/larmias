{
    "name": "larmias/engine",
    "description": "A base engine library for Larmias.",
    "type": "library",
    "keywords": [
        "larmias",
        "engine",
        "server"
    ],
    "homepage": "https://larmias.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "larmdcm",
            "email": "mdcm98@163.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "psr/event-dispatcher": "^1.0",
        "larmias/contracts": "^1.0",
        "larmias/support": "^1.0",
        "larmias/facade": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Larmias\\Engine\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "larmias": {
            "providers": [
                "Larmias\\Engine\\Providers\\EngineServiceProvider"
            ]
        }
    }
}